<% if @task.item_id? %>
	$("#task-"+<%= @task.id %>).parents(".tasks-list").html("<%= j render @task.item.tasks.incomplete.priority_sort %>");
<% else %>
	$("#task-"+<%= @task.id %>).parents(".tasks-list").html("<%= j render @task.project.tasks.general.incomplete.priority_sort %>");
<% end %>

$("#task-"+<%= @task.id %>).addClass("animated fadeIn");
