<table class="table">
  <thead>
    <tr>
      <th width="25%">Project Name</th>
      <th width="25%">Assigned</th>
      <th width="10%">Start Date</th>
      <th width="10%">Due Date</th>
      <th width="10%">End Date</th>
      <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <% product.projects.each do |project| %>
      <tr>
        <td><%= link_to project.name, project %></td>
        <td><%= project.users.first ? project.users.uniq.map {|u| u.name}.join(", ") : "None" %></td>
        <td><%= print_date(project.created_at, "s") if project.created_at %></td>
        <td><%= print_date(project.due_date, "s") if project.due_date %></td>
        <td><%= print_date(project.end_date, "s") if project.end_date %></td>
        <td><%= project.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>