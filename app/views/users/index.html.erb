<div class="panel panel-default">

  <div class="panel-heading">
    <h4><i class="fa fa-users"></i> Team
  </div>

  <div class="panel-body">

    <% for user in @users %>

      <h4><%= image_tag user.profile_photo.url(:small), class: "profile-photo", style: "width: 50px; height: 50px; margin-right: 10px;" %> <%= link_to user.name, user %></h4>
      <%= render partial: "users/projects_table", :locals => {:projects => user.projects.incomplete.uniq} %>
      <hr><br><br>

    <% end %>

  </div>

</div>


<br>