.col-md-12
 
  .panel.panel-default

    .panel-heading
      %h4
        %i.fa.fa-users
          Team
          
    .panel-body

      %table.table.table-hover.table-responsive
        %thead
          %tr
            %th id
            %th Name
            %th Email

        %tbody
          - @users.each do |user|
            %tr
              %td= user.id
              %td= user.name
              %td= user.email

