<table class="table">
	<thead>
		<tr>
       	<th width="15%">Product</th>
         <th width="30%">Project</th>
         <th width="15%">Assigned By</th>
         <th width="10%">Start Date</th>
         <th width="10%">Due Date</th>
         <th width="10%">End Date</th>
         <th width="10%">Status</th>
    	</tr>
	</thead>
	<tbody>

			<% for project in projects %>
							<tr>
				<td><%= link_to project.product.name, project.product %></td>
				<td><%= link_to project.name, project %></td>
				<td><%= project.assigned_by %></td>
				<td><%= print_date(project.created_at, "s") %></td>
				<td><%= print_date(project.due_date, "s") if project.due_date %></td>
				<td><%= print_date(project.end_date, "s") if project.end_date %></td>
				<td><%= project.status %></td>
			</tr>
			<% end %>


	</tbody>
</table>